{
  "root": true,
  "globals": {
    "defineProps": "readonly"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "plugins": ["prettier"],
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    // "vue/html-self-closing": ["error"],
    "vue/multi-word-component-names": ["off"],
    "no-unused-vars": ["off"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
